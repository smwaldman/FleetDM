<Replace>
  <!-- Name of SCEP node -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">node</Format>
    </Meta>
  </Item>
  <!-- Key Usage - keep default for Okta -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/KeyUsage</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">int</Format>
    </Meta>
    <Data>160</Data>
  </Item>
  <!-- Key Length - min 2048 for Okta -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/KeyLength</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">int</Format>
    </Meta>
    <Data>2048</Data>
  </Item>
  <!-- Hash Algorithm - keep default for Okta -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/HashAlgorithm</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>SHA-2</Data>
  </Item>
  <!-- CN - keep default for Okta -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/SubjectName</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>CN=$FLEET_VAR_HOST_UUID Okta managementAttestation</Data>
  </Item>
  <!-- Extended Key Usage - keep default for Okta -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/EKUMapping</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>1.3.6.1.4.1.311.10.3.12+1.3.6.1.4.1.311.10.3.4+1.3.6.1.4.1.311.20.2.2+1.3.6.1.5.5.7.3.2</Data>
  </Item>
  <!-- SCEP Server URL -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/ServerURL</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>$FLEET_SECRET_SCEP_URL</Data>
  </Item>
  <!-- SCEP Challenge - SECRET CANNOT CONTAIN UNDERSCORES!!!!!!! -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/Challenge</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>$FLEET_SECRET_SCEP_SECRET</Data>
  </Item>
  <!-- SCEP CA Thumbprint - Download Okta CA (if using) and specify thumbprint here -->
  <Item>
    <Target>
      <LocURI>./User/Vendor/MSFT/ClientCertificateInstall/SCEP/{{your_node}}/Install/CAThumbprint</LocURI>
    </Target>
    <Meta>
      <Format xmlns="syncml:metinf">chr</Format>
    </Meta>
    <Data>$FLEET_SECRET_SCEP_CA_THUMBPRINT</Data>
  </Item>
</Replace>
